
@{
    ViewBag.Title = "Delete";
}

<h1>Delete Program</h1>

<p>Are you sure you want to delete the program: <strong>@Model.ProgramName</strong>?</p>

<form method="post" action="@Url.Action("DeleteConfirmed", new { id = Model.ProgramId })">
    @Html.AntiForgeryToken()
    <button type="submit" class="btn btn-danger">Delete</button>
    <a href="@Url.Action("ProgramList")" class="btn btn-secondary">Cancel</a>
</form>

